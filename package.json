{
  "name": "typegrid.js",
  "version": "0.0.1",
  "description": "function for graphic designers & designing in browser.",
  "main": "index.js",
  "scripts": {
    "clean/js": "rimraf dist/assets/js && mkdir -p dist/assets/js",
    "js/webpack": "webpack",
    "js/jsonCopy":"node -e \"require('fs').copyFileSync('src/js/typegrid.json', 'dist/assets/js/typegrid.json')\"",
    "js": "npm run clean/js && npm-run-all -s js/*",
    "watch/js": "watch 'npm run js' ./src/js",
    "watch/server": "browser-sync start -s dist -f 'src, **/*.html'",
    "watch": "npm-run-all -p watch/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://ytkwsm@bitbucket.org/ytkwsm/typegrid.js.git"
  },
  "keywords": [
    "Graphic",
    "Design",
    "Web",
    "Design",
    "Layout",
    "Typography",
    "Grid",
    "Layout"
  ],
  "author": "ytkwsm",
  "license": "MIT",
  "homepage": "https://bitbucket.org/ytkwsm/typegrid.js#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-regenerator": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.23.6",
    "core-js": "^3.6.5",
    "cpx": "^1.5.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.0.1",
    "luxon": "^1.25.0",
    "markdown-it": "^11.0.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.0.2",
    "postcss-cli": "^8.0.0",
    "terser-webpack-plugin": "^4.2.2",
    "watch": "^1.0.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-stream": "^4.0.3"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  }
}
